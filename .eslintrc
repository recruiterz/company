{
  "root": true,
  "extends": ["@stillmine", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "globals": {
    "__PATH_PREFIX__": "readonly"
  },
  "overrides": [
    {
      "files": ["**/*.ts", "**/*.tsx"],
      "parser": "@typescript-eslint/parser",
      "plugins": ["@typescript-eslint"],
      "rules": {
        "no-undef": "off",
        "no-unused-vars": "off"
      }
    }
  ]
}
